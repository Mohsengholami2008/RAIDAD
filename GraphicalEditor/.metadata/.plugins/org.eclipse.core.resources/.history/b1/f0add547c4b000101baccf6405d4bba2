      <role xsi:type="mLmodel:Target" name="Species"/>
    </cycle>
    <cycle xsi:type="mLmodel:Duplicates" after="//@process/@cycle.2" before="//@process/@cycle.0">
      <duplicates_argumans xsi:type="mLmodel:Keep"/>
    </cycle>
    <cycle xsi:type="mLmodel:Simple_Imputer" after="//@process/@cycle.3" before="//@process/@cycle.1" regular="//@process/@cycle.0/@role.0 //@process/@cycle.0/@role.1 //@process/@cycle.0/@role.2 //@process/@cycle.0/@role.3">
      <simple_imputer_arguments xsi:type="mLmodel:Strategy"/>
    </cycle>
    <cycle xsi:type="mLmodel:KNN" after="//@process/@cycle.4" before="//@process/@cycle.2"/>
    <cycle xsi:type="mLmodel:Cross_Validation" before="//@process/@cycle.3" precision="true" f1_score="true" number_of_folds="10">
      <plots xsi:type="mLmodel:Bar"/>
      <plots xsi:type="mLmodel:Scatter"/>
    </cycle>
